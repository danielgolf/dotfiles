#!/bin/zsh
xrandr --output eDP1 --primary --mode 2560x1440 --pos 0x0 --rotate normal --dpi 128 --output DP1 --off --output DP2 --off --output DP2-1 --off --output DP2-2 --off --output DP2-3 --off --output HDMI1 --off --output VIRTUAL1 --off
$HOME/.fehbg
$HOME/.config/polybar/start_polybar_normal.sh
xautolock -detectsleep -time 10 -locker "systemctl suspend" &!
xset dpms 0 0 0
